<h1>NEW Blogger</h1>


<% if @errors %>
    <% @errors.each do |error| %>
        <%= error %> <br>
    <% end %>
<% end %>

<%= form_for @blogger do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name %><br>

    <%= f.label :bio %> 
    <%= f.text_field :bio %><br>
    
    <%= f.label :age %> 
    <%= f.number_field :age %><br>

    <%= f.submit "Done!" %> 

<% end %>
